{
  "name": "peertube-plugin-browser-live",
  "version": "1.0.0",
  "description": "Stream directly from browser to PeerTube without OBS.",
  "author": "joeaa17",
  "license": "AGPL-3.0",
  "main": "./dist/server/main.js",
  "library": "./dist/server/main.js",
  "clientScripts": [
    {
      "script": "dist/client/index.js",
      "scopes": ["livecreate"]
    }
  ],
  "peertube": {
    "version": ">=7.0.0"
  },
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "babel src/server -d dist/server",
    "build:client": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.13.0",
    "ffmpeg-static": "^4.4.1",
    "fluent-ffmpeg": "^2.1.2",
    "wrtc": "^0.4.7",
    "uuid": "^9.0.0",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.4",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}